<div class="page-header">
	<h1>Teacher Review for <%= @project.project_name %> </h1>
</div>

<div class="jumbotron" style = "background:#F8F8F8;text-align:center">
	    <h2>Teacher Review</h2>
	    <h3><%= @project_instance.school %> in <%= @project_instance.town %></h3>
    <br>

    <div class="row">
  		<div class="col-md-4"><p><strong>Overall Rating:</strong> <%= @review.rating %></p></div>
  		<div class="col-md-4"><p><strong>Feasibility:</strong> <%= @review.feasibility %></p></div>
  		<div class="col-md-4"><p><strong>Learning Effectiveness: </strong><%= @review.learning_effectiveness %></p></div>
	</div>
	<p style = "text-align:center"> <%= @review.review_text %> </p>
</div>

